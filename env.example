# Environment Variables for Hackathon Signal

# Telegram Bot Configuration (REQUIRED)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHANNEL_ID=your_telegram_channel_id_here

# Telegram Settings (OPTIONAL - defaults provided)
TELEGRAM_ENABLED=true
TELEGRAM_MAX_TWEETS_TO_SEND=15
TELEGRAM_MIN_SCORE_TO_SEND=0.3

# API Keys (for data ingestion)
RAPID_API_KEY=your_rapidapi_key_here

# OpenAI Configuration (for LLM-generated hackathon titles)
OPENAI_API_KEY=your_openai_api_key_here 